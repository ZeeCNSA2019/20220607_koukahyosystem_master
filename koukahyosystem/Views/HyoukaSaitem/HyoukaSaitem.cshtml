            text-align: right;
        }*/

        .date_tbl td {
            border: none;
        }


        .tab-content {
            height: 51vh;
            overflow: auto;
        }

        .scroll {
            border: none;
            border-collapse: separate;
            border-spacing: 0;
            table-layout: fixed;
            border: 0.5px solid #cccccc;
        }

            .scroll th, .scroll td {
                height: 30px;
                text-align: center;
                border: 0.5px solid #cccccc;
            }

            .scroll thead th {
                background-color: rgb(189, 233, 242);
                position: -webkit-sticky;
                position: sticky;
                top: 0;
                z-index: 2;
