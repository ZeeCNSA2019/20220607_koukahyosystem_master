            border: 1px solid black;
            box-shadow: inset 1px 1px 1px 1px #ddd,inset 1px 1px 1px 1px #ddd;
        }*/

        .headercss {
            position: relative;
            width: auto;
            float: none;
            /*border: 1pt solid black;*/
        }

        .tab {
            overflow: hidden;
            /*border: 1px solid #ccc;*/
            background-color: #f1f1f1;
        }

            .tab button {
                background-color: inherit;
                float: left;
                border: none;
                /*outline: none;*/
                cursor: pointer;
                padding: 14px 16px;
                transition: 0.3s;
                font-size: 17px;
                width: 80px;
