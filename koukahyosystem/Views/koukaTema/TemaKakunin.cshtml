            text-align: right;
            border-radius: 5px;
            border-radius: 5px;
            padding: 2px;
            border-color: lightgray;
            border-style: inset;
            border-left-color: ghostwhite;
            border-top-color: ghostwhite;
        }

        .txt_tasseiritsu {
