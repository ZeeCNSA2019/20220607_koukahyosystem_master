    <script>
        function isSpecialKey(event) {

            var regex = new RegExp("^[0-9]+$");
            var key = String.fromCharCode(!event.charCode ? event.which : event.charCode);

            if (!regex.test(key)) {

                event.preventDefault();
                return false;
            }
            else {
                return true;
            }
        }

        function edValueKeyPress(id) {

            var tas_val = document.getElementById(id).value;
            tas_val = tas_val.replace("%", "");

            if (isNaN(tas_val)) {
                document.getElementById(id).value = "";
                return false;
            }

            if (tas_val.trim().length > 0) {
                if (tas_val > 100) {
                    tas_val = 100;
                }
                document.getElementById(id).value = tas_val + "%";
            }
            else {
                if (tas_val > 100) {
                    tas_val = "100";
                }
                document.getElementById(id).value = "";
                if (tas_val.trim().length == 0) {
                    document.getElementById(id).value = "";
                }
                if (tas_val.trim().length > 0) {
                    document.getElementById(id).value = tas_val + "%";
                }
            }

            //if (tas_val.trim().length > 0) {
