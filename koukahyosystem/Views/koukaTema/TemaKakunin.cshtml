                margin-left: 25px;
            }

            .lbl_leader_kakutei {
                text-align: left;
            }

            .text_area1 {
                height: auto;
            }
        }

        @@media only screen and (max-width: 480px) {

            #div_btn {
                float: unset;
            }
            .div_dropdown {
                display: block;
            }

            .btn_hozone {
                margin-left: 25px;
            }

            .lbl_leader_kakutei {
                text-align: left;
            }

