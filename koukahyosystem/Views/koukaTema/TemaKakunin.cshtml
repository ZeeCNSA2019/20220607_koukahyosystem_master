</head>
<body>
    @if (Session["isAuthenticated"] != null)
    {
        <div id='divElement' class="panel panel-info">

            @using (Html.BeginForm("TemaKakunin", "KoukaTema"))
            {
                <div class="panel-heading text-center" style="background-color: rgb(34, 148, 172); width:100%;">
                    <div class="container">
                        <h4 style="color:white">目標設定（100%の力で70%達成できる目標を作る。）</h4>
                    </div>
                </div>

                <div class="row" style="margin-top:10px">
                    <div class="col-lg-3 col-sm-4">
                        <label class="lbl_year">年度</label>
                        <button id="btnPrevious" name="btnPrevious" value=@Model.year class="btncss btn_year"> < </button>
                        @Html.DropDownListFor(m => m.year, Model.yearList, new { @id = "year", @class = "dp_year",onchange = "return OnChanged()" })
                        <button id="btnNext" name="btnNext" value=@Model.year class="btncss btn_year"> > </button>
                        <button id="btnSearch" name="btnSearch" value="add" style="position: absolute; visibility:hidden">add</button>
                    </div>

                    <div class="col-lg-3 col-sm-4" id="div_dropdown">
                        <label class="lbl_shainName">社員名</label>
                        @Html.DropDownListFor(m => m.shain_name, Model.shain_list, new { @id = "shain",@class="dp_shain",onchange = "return OnChangedSelect()", @autofocus = true })

                        <button id="btnSelect" name="btnSelect" value="select" style="position: absolute; visibility:hidden">select</button>
                        <br />

                        @if (TempData["master_kaiso"] != null)
                        {
                            <label class="lbl_busho">@TempData["master_kaiso"].ToString()</label>
                        }
                        
                        @Html.CheckBoxFor(model => model.check, new { id = "leader_check", style = "margin-left:5px;" })

                        <button id="btnCheck" name="btnCheck" value="check" style="position: absolute; visibility:hidden">check</button>

                    </div>
                    
                    <div class="col-lg-3 col-sm-4">
                        <label class="div_lbl_leader_haiten">配点</label>
                        @Html.Label(TempData["master_haiten"].ToString(), new { id = "master_haiten", style = "color:black;font-weight: normal !important;margin-left:5px;" })

                        @if (Model.tensuu_hide == false)
                        {
                            <label class="div_lbl_tensuu">点数</label>
                            @Html.Label(TempData["tensuu"].ToString(), new { style = "color:black;font-weight: normal !important;margin-left:5px;" })
                        }

                        @Html.TextBoxFor(model => model.shain_name, new { @id = "txt_shainName", style = "width:0px;text-align:center;visibility:hidden" })

                        @if (Model.shain_name != "")
                        {
                            <input type="text" id="txt_shain" name="txt_shain" value=@Model.shain_name style="width:0px;text-align:center;visibility:hidden" />
                        }
                        else
                        {
                            <input type="text" style="width:0px;text-align:center;visibility:hidden" />
                        }
                    </div>
                    
                    <div class="col-lg-3" id="div_btn">

                        <button type="submit" id="btn_hozone" name="btn_hozone" value="hozone" class="btncss leader_btn_hozone"></button>

                        <button type="submit" id="btn_kakutei" name="btn_kakutei" value="kakutei" class="btncss leader_btn_kakutei"></button>

                        <button type="submit" id="btn_sashimodoshi" name="btn_sashimodoshi" class="btncss leader_btn_sashimodoshi" value="sashimodoshi">差戻し</button>

                    </div>

                </div>

