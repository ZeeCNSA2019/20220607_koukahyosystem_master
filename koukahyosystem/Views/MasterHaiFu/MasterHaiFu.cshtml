    <script>

    </script>
    <script>

        function SelectedIndexChanged() {
            $('#searchBtn').click();
        }

        $(function () {

             $('tfoot a').click(function () {

                 var page = this.href.match(/page=([0-9])+/)[1];
                 page = page - 1;
                 $("#pgindex").val(page);
                 $('#pageindex_btn').click();
                 return false;
             });

             $('th a').click(function () {
                 var link = this.href;
                 var linkArr = link.split('?');

                 if (linkArr.length == 2) {
                     var linkfir = linkArr[1].split('&');
                     if (linkfir.length == 2) {
                         //sort
                         var sortArr = linkfir[0].split('=');
                         var sort = sortArr[sortArr.length - 1];
                         //alert(sort);
                         $("#sort").val(sort);
                         $('#sortOrder_btn').click();
                         
                         return false;
                     }
                 }
             });

             if ('@Model.fpopup' == "True") {

                    $('#myModal').modal('show');
                    return true;
                }
                else {
                    $('#myModal').modal('hide');
             }

             @*if ('@Model.fpermit' == "False") {
                 $('#newEditBtn').attr('disabled', 'disabled');
                 $('#newEditBtn').css("opacity", "0.6");
                 $('#newEditBtn').css("cursor", "not-allowed");
                }*@


            });
    </script>
    <script>
        $(document).ready(function () {
            
            //テーブルのセルをクリックすると、ラジオボタンをチェックする
            $('#DataTable tr td').click(function (event) {

                var col = $(this).index();
              
                if (col > 5 && col < 9 ) {
                    var rowIndex = $(this).parent().index();
                    var radcol = col- 6;
                    var chk = $(this).closest("tr").find("input:radio").get(radcol);
                  
                    if (event.target != chk) {
                        if (chk.checked == false) {
                            chk.checked = !chk.checked;
                            //td radion button is change                                                    
                            //$(this).closest("tr").find('td:eq(10) input').val("1");
                           
                        }
                    }
                }

            });

            $('#DataTable tbody tr td input').change(function () {
                var col =  $(this).parent().index();
               
                if (col == 4) {
                    $(this).closest("tr").find('td:eq(10) input').val("1");
                }                
            });
        });
    </script>
    <script>
        
        function msgFunction() {
           
            //if (fchanged == "1") {
                
            //}

            var fnodata = true;
            var n1 = document.getElementById("DataTable").rows.length;
            var fmsg = false;

            for (var i = 1; i < n1; i++) {
                var fhyouka = document.getElementById('DataTable').rows[i].cells[9].children[0].value;
                var fhenkou = document.getElementById('DataTable').rows[i].cells[10].children[0].value;
               
                if (fhyouka === "1" && fhenkou === "1") {                  
                    fmsg = true;
                    break;
                }
                else {                   
                    fmsg = false;
                }
            }

           
            if (fmsg == true) {
                var r = confirm("評価中の目標評価がリセットされますがよろしいですか？");
                if (r == false) {                    
                        event.preventDefault();
                    }               
            }

            for (var i = 1; i < n1; i++) {
                var mark1 = document.getElementById('DataTable').rows[i].cells[2].children[0].value;
                var mark2 = document.getElementById('DataTable').rows[i].cells[3].children[0].value;
                var mark3 = document.getElementById('DataTable').rows[i].cells[4].children[0].value;
                var mark4 = document.getElementById('DataTable').rows[i].cells[5].children[0].value;
                $(document.getElementById('DataTable').rows[i].cells[2].children[0]).css('borderColor', '');
                $(document.getElementById('DataTable').rows[i].cells[3].children[0]).css('borderColor', '');
                $(document.getElementById('DataTable').rows[i].cells[4].children[0]).css('borderColor', '');
                $(document.getElementById('DataTable').rows[i].cells[5].children[0]).css('borderColor', '');
                $(document.getElementById('DataTable').rows[i].cells[2].children[0]).css('border', 'none');
                $(document.getElementById('DataTable').rows[i].cells[3].children[0]).css('border', 'none');
                $(document.getElementById('DataTable').rows[i].cells[4].children[0]).css('border', 'none');
                $(document.getElementById('DataTable').rows[i].cells[5].children[0]).css('border', 'none');

                if (mark1.trim().length == 0) {
                    $(document.getElementById('DataTable').rows[i].cells[2].children[0]).css('border', '1px solid');
                    $(document.getElementById('DataTable').rows[i].cells[2].children[0]).css('borderColor', '#f3172d');
                    fnodata = false;
                }

                if (mark2.trim().length == 0) {
                    $(document.getElementById('DataTable').rows[i].cells[3].children[0]).css('border', '1px solid');
                    $(document.getElementById('DataTable').rows[i].cells[3].children[0]).css('borderColor', '#f3172d');
                    fnodata = false
                }

                if (mark3.trim().length == 0) {
                    $(document.getElementById('DataTable').rows[i].cells[4].children[0]).css('border', '1px solid');
                    $(document.getElementById('DataTable').rows[i].cells[4].children[0]).css('borderColor', '#f3172d');
                    fnodata = false
